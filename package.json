{
  "name": "babel-plugin-react-native",
  "version": "1.0.1",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "https://github.com/christophehurpeau/babel-plugin-react-native.git",
  "homepage": "https://github.com/christophehurpeau/babel-plugin-react-native",
  "type": "commonjs",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "packageManager": "yarn@3.1.1",
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "generate:test-coverage": "rm -Rf docs/coverage/ ; NODE_ENV=production jest --coverage --coverageReporters=pob-lcov-reporter --coverageDirectory=docs/coverage/",
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:eslint": "eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet *.{js,cjs,mjs} lib",
    "lint:prettier": "pob-root-prettier --check .",
    "lint:prettier:fix": "pob-root-prettier --write .",
    "postinstallDev": "pob-root-postinstall",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "commitlint": {
    "extends": [
      "@pob/commitlint-config"
    ]
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "<rootDir>/lib/**/__tests__/**/*.?(m)js",
      "<rootDir>/lib/**/*.test.?(m)js"
    ],
    "collectCoverageFrom": [
      "lib/**/*.?(m)js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testEnvironment": "node"
  },
  "pob": {},
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@pob/commitlint-config": "4.2.0",
    "@pob/eslint-config": "49.1.0",
    "@pob/root": "6.18.0",
    "@types/jest": "27.4.1",
    "eslint": "8.14.0",
    "jest": "28.0.3",
    "nightingale-unit-testing": "12.1.0"
  }
}
